<div class="container-fluid">
    <div class="row">

        <a href="/admin/add" class="btn btn-success m-auto">Add-Products</a>

        <div class="col-md-12">

            <table class="table  mt-5" style="text-align: center;">
                <thead class="">
                    <tr>
                        <th scope="col">No.</th>
                        <th scope="col">Name</th>
                        <th scope="col">Discription</th>
                        <th scope="col">Price</th>
                        <th scope="col">Image</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each data}}
                    <tr>

                        <td>{{@index}}</td>
                        <td>{{this.Name}}</td>
                        <td>{{this.discription}}</td>
                        <td>{{this.price}}</td>
                        <td><img src="/uploded-image/{{this._id}}.jpg" alt="" style="width: 200px; height: 200px;"></td>


                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>