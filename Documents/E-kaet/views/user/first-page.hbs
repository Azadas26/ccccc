<div class="container-fluid">
    <div class="row">
        {{#each data}}
        <div class="col-sm-4 col-md-3 mt-5">
            <div class="card m-auto" style="width: 18rem; height: 27rem; text-align: center;">
                <div class="mt-4">
                    <img class="card-img-top m-auto img-fluid" src="/uploded-image/{{this._id}}.jpg"
                        alt="Card image cap" style="width: 200px; height: 200px;">
                    <div class="card-body">
                        <h5 class="card-title">{{this.Name}}</h5>
                        <p class="card-text">Some quick example text to build on the card title and make up the bulk of
                            the card's
                            content.</p>
                        <a href="#" class="btn btn-primary">Go somewhere</a>
                    </div>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>